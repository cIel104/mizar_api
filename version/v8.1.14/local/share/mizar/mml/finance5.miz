      A` /\ {w where w is Element of Omega: tau.w<=t}
      proof
       for j being object holds
        j in A` /\ {w where w is Element of Omega: tau.w<=t} implies
         j in {w where w is Element of Omega: tau.w<=t} \
          (A /\ {w where w is Element of Omega: tau.w<=t})
       proof
