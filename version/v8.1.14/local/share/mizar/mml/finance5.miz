    not c in [.0+1,+infty.] by XXREAL_1:1; then
    not c in (ext_left_closed_sets(0)).1 by Def4000;
    hence thesis by Y,PROB_1:13;
   end;
   suppose S1: c >= 0;
    set finerg=[\ c+2*1 /];
