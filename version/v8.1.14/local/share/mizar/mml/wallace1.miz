    set C = STC0ICirc(x1,x2,x3,x4,x5,x6,x7);
    set S1 = STC0IIStr(x1,x2,x3,x5,x6,x7);
    set C1 = STC0IICirc(x1,x2,x3,x5,x6,x7);
    set A1out = GFA0AdderOutput(x1,x2,x3);
