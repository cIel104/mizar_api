    InputVertices STC0Str(x1,x2,x3,x4,x5,x6,x7) is without_pairs
  proof
    let x1,x2,x3,x4,x5,x6,x7 be non pair set;

    InputVertices STC0Str(x1,x2,x3,x4,x5,x6,x7) = {x1,x2,x3,x4,x5,x6,x7}
