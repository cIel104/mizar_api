     = xor2.(s*<*C1out,C2out*>) by ThSTC0S7,FACIRC_1:35
    .= xor2.<*aC1, aC2*> by A1,A2,A3,FINSEQ_2:125
    .= aC1 'xor' aC2 by FACIRC_1:def 4;
  end;

LmSTC0S15S1s6a: :: Lm6a (step-5, C1C2x)
