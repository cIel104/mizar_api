    then
A18: rng(s^\k)c=L by A12,XBOOLE_1:1;
A19: rng s c=dom f2 by A10,Lm2;
    then
A20: rng(s^\k)c=dom f2 by A12,XBOOLE_1:1;
    now
      set t=|.g.|+1;
