A4: 0<g1 and
A5: for r st x0<r ex r1 st r1<r & x0<r1 & r1 in dom f & |.f.r1-g.|>= g1 by A3;
    defpred X[Nat,Real] means x0<$2 & $2<x0+1/($1+1) & $2 in
    dom f & g1<=|.f.($2)-g.|;
A6: now
      let n be Element of NAT;
