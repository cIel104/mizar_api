A26:    lim(seq^\k)= x0 by A10,A11,SEQ_4:20;
A27:    dom f2/\].x0-r1,x0.[c=dom f2 by XBOOLE_1:17;
        then rng(seq^\k)c=dom f2 by A25,XBOOLE_1:1;
