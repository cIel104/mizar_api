AA8:    x in dom(f) /\ dom(g) by XBOOLE_0:def 1;
        x in dom(f) by AA8, XBOOLE_0:def 4; then
a9:     x in (the carrier' of CPNT.i) \ Outbds(CPNT.i) by AA4, AA6;
