A20:    b.n = 0;
A21:     n in NAT by ORDINAL1:def 12;
A22:    (h + c).n = h.n + c.n by SEQ_1:7
          .= h.n + f.x0 by A19,A21;
        f.(a.n + b.n) = f.((f").(f.x0)) by A9,A20,SEQ_1:57
          .= f.x0 by A8,FUNCT_1:34;
