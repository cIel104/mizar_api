  :Def6:
  OrderingSelector in dom G & G.OrderingSelector
  is Enumeration of the_Vertices_of G;
end;

Lm1:
  not OrderingSelector in _GraphSelectors
    by GLIB_000:def 5, GLIB_000:1, ENUMSET1:def 2;

registration
