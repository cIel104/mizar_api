    hence v1 in dom F1_V by A5, FUNCT_4:10, TARSKI:def 3;
    thus v3 in dom F1_V by A2, A5, FUNCT_4:10, TARSKI:def 3;
    thus F1_V.v1 = (v1 .--> v2).v1 by A5, A10, FUNCT_4:13
