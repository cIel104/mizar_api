    G == G0 by Th3;
    then the_Vertices_of G = the_Vertices_of G0 by GLIB_000:def 34;
    then reconsider f as ManySortedSet of the_Vertices_of G0;
    take f;
    OrderingSelector <> VLabelSelector by GLIB_003:def 3;
