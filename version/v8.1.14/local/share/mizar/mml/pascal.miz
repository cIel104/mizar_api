Lm30:
  MABE = <* <* 1,0,0 *>, <* 0,1,0 *>,    <* e1,e2,e3 *> *> &
  MACF = <* <* 1,0,0 *>, <* 0,0,1 *>, <* f1,f2,f3 *> *> &
