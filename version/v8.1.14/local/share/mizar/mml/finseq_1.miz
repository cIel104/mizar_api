registration
  let f,g be ext-real-valued FinSequence;
  cluster f^g -> ext-real-valued;
  coherence;
end;

registration
  let n be natural Number;
  cluster <*n*> -> natural-valued;
  coherence
  proof
    set f = <*n*>;
    let x be object such that
A1: x in dom f;
    dom f = {1} by Def8,Th2;
    then x = 1 by A1,TARSKI:def 1;    
    hence thesis by Def8;
  end;
end;

