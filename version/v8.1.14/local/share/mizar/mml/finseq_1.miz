registration
  cluster -> natural-valued for FinSequence of NAT;
  coherence;
  cluster -> integer-valued for FinSequence of INT;
  coherence;
  cluster -> rational-valued for FinSequence of RAT;
  coherence;
  cluster -> real-valued for FinSequence of REAL;
  coherence;
  cluster -> complex-valued for FinSequence of COMPLEX;
  coherence;
  cluster -> ext-real-valued for FinSequence of ExtREAL;
  coherence;
end;

:: registration
::   let D be non empty set;
::   let x1 be Element of D;
::   cluster <*x1*> -> D-valued;
::   coherence;
::   let x2 be Element of D;
::   cluster <*x1,x2*> -> D-valued;
::   coherence;
::   let x3 be Element of D;
::   cluster <*x1,x2,x3*> -> D-valued;
::   coherence;
:: end;
