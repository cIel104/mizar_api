              .= Den((the connectives of S)/.12, A).<*fj,ia,b*>
              by A4,A2,A33
              .= Den((the connectives of S)/.12, Q).<*fi,ii,a*>
              by A35,FINSEQ_3:125,EQUATION:19
              .= (fi,ii)<-a by A34,SUBSET_1:def 8;
