        assume A8:P[i];
        A9: |.B.(i+1).| <= |.B.i.| -1 by A5;
        |.B.i.| -1 <= (|.B.0 .| - i) -1 by A8,XREAL_1:9;
        hence P[i+1] by A9,XXREAL_0:2;
      end;
      A10: for i be Nat holds P[i] from NAT_1:sch 2(A6,A7);
      |. B. |.B.0 .| .| <=|.B.0 .| - |.(B.0).| by A10;
