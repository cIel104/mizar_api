        reconsider u=f2/.m1 as Point of TOP-REAL 2;
A81:    w5<=len f2 by A19,A73,A80,XREAL_1:6;
        then f/.m = f2/.w5 by A80,A79,SEQ_4:136;
        then
A82:    y.m=u`2 by A16,A67,A72,GOBOARD1:def 2;
A83:    m1 in dom f2 by A79,A81,FINSEQ_3:25;
