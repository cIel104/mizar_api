    A2: for n being Nat holds s1.n=IFGT(n,k,0,1) and
    A3: for n being Nat holds s2.n=IFGT(n,k,0,1);
     let n be Element of NAT;
     s1.n=IFGT(n,k,0,1) & s2.n=IFGT(n,k,0,1) by A2,A3;
