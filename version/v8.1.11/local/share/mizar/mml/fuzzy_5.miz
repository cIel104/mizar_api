    B12: f.x1 = fa.x1 by FUNCT_4:11,Dfp,XXREAL_1:236,FF
    .= AffineMap (a,b).x1 by FUNCT_1:49,B1,XXREAL_1:233;
    per cases;
    suppose C1: x2 < (q-b)/(a+p);
     C13: f.x2 = fa.x2 by FUNCT_4:11,Dfp,XXREAL_1:236,C1,FF
