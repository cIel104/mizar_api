 D2: dom f2 = [.a,b.] by FUNCT_2:def 1;
 D3: dom f3 = [.b,c.] by FUNCT_2:def 1;
 D4: dom f4 = [.c,d.] by FUNCT_2:def 1;
 AA2: a in dom f2 by D2,A1;
 AA4: dom (f3 +* f4)= (dom f3 \/ dom f4) by FUNCT_4:def 1
