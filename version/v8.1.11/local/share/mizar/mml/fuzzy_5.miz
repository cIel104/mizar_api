TrapezoidalFS (a,b,c,d). b = 1 &
TrapezoidalFS (a,b,c,d). c = 1 &
TrapezoidalFS (a,b,c,d). d = 0
proof
 let a,b,c,d be Real;
 assume A1: a<b & b<c & c < d; then
 AA1: a in [.a,b.];
 set f1= AffineMap (0,0) | (REAL \ ].a,d.[);
 set f2= (AffineMap (1/(b - a),- (a / (b - a))) | [.a,b.]);
 set f3= AffineMap (0,1) | [.b,c.];
