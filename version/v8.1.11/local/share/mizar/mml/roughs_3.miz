      end;
    end;
    set HH = UAp GeneratedRelStr H;
    for X being Subset of R holds HH.X = H.X
    proof
      let X be Subset of R;
PS:   HH.X c= H.X
      proof
        let x be object;
        assume
a1:     x in HH.X;
        reconsider XX = X as Subset of GeneratedRelStr H;
a2:     x in UAp XX by ROUGHS_2:def 11,a1;
