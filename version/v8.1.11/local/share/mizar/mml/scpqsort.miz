A73:    yd >= 1 by A42,A48,XREAL_1:47,XXREAL_0:2;
A74:    t1.intpos(k3+1)=m+yn by A41,A64,A60,SCMPDS_4:8;
        ym+(1+k) >= y1+(1+k) by A55,XREAL_1:6;
