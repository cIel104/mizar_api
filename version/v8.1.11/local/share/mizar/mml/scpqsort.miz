 holds IExec(WH2,Q,t).a=0 & IExec(WH2,Q,t).
  a1=t.a1 & IExec(WH2,Q,t).a7=0 & IExec(WH2,Q,t).a2=t.a2
   & IExec(WH2,Q,t).a4=t.a4 &
  (for i be Nat st i >= 8
 holds IExec(WH2,Q,t).intpos i=t.intpos i) &
 ex m5,mE3 be Nat st m5=IExec(WH2,Q,t).a5
