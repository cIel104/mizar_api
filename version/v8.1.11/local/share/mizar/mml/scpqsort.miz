A34: t6.intpos mp=t.intpos mp by A33;
A35: mp+1 <> mp+2;
A36: mp+3 <> mp+2;
A37: mp+3 >= 8+3 by A24,XREAL_1:6;
    then
A38: mp+3 > 1 by XXREAL_0:2;
    mp+3 > 1 by A37,XXREAL_0:2;
    then
A39: t6.SBP=m1 by A21,A33;
    then
A40: DataLoc(t6.SBP,pn+1)=intpos (mp+1) by SCMP_GCD:1;
A41: now
      let i be Nat;
      assume
A42:  i<>mp+1;
