  (for i be Nat st t.a3 <= i & i < mE3
 holds IExec(WH2,Q,t).intpos md >
 IExec(WH2,Q,t).intpos i)
 & (m5=0 or IExec(WH2,Q,t).intpos md <=
  IExec(WH2,Q,t).intpos mE3);
