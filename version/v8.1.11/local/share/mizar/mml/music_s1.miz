  proof
    heptatonic_pythagorean_scale(HPS,frequency).5
      = spiral_of_fifths(HPS,frequency,Fourth(HPS,frequency)).2
    by Def26;
    hence thesis;
  end;

  func hepta_5(HPS,frequency) -> Element of HPS equals
