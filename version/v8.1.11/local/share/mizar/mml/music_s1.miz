  let frequency be Element of HPS;

  func hepta_fondamental(HPS,frequency) -> Element of HPS equals
    heptatonic_pythagorean_scale(HPS,frequency).1;
  coherence
  proof
    heptatonic_pythagorean_scale(HPS,frequency).1
