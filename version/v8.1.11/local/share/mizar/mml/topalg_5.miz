              .= (CircleMap*Fn2).x by A281,A342,A392,FUNCT_1:49
              .= CircleMap.(Fn2.x) by A294,A342,A392,FUNCT_1:13
              .= f.(Fn2.x) by A385,A386,A391,FUNCT_1:49
              .= (f*Fn2).x by A294,A342,A392,FUNCT_1:13
