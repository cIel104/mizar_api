definition
  let Omega be non empty set;
  let RV be Function of Omega,REAL;
  let w be Element of Omega;
  func SetForCall-Option(RV,w) -> Element of REAL equals :Def89:
  RV.w if RV.w>=0 otherwise 0;
  correctness by NUMBERS:19;
