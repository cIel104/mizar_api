 requirements REAL, NUMERALS, SUBSET, BOOLE, ARITHM;
 definitions TARSKI, XBOOLE_0;
 equalities XBOOLE_0, VALUED_1, PROB_1, LIMFUNC1;
 expansions TARSKI, LIMFUNC1;
