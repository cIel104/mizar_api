registration
let R be non degenerated Ring,
    S be RingExtension of R;
let T be Subset of S;
cluster RAdj(R,T) -> non degenerated;
coherence
proof
set RA = RAdj(R,T);
0.RA = 0.S & 1.RA = 1.S by dRA;
hence 0.RA <> 1.RA;
end;
