registration
let F be Field,
    R be RingExtension of F;
let T be Subset of R;
cluster RAdj(F,T) -> field-containing;
coherence
proof
ex E being Field st E is Subring of RAdj(F,T)
  proof
  take F;
  thus thesis by RAsub;
  end;
hence thesis;
end;
