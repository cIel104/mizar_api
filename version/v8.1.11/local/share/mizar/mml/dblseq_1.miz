    hence Rseq.(n,m) = r by FUNCOP_1:70;
   end;
a3:now let e be Real;
    assume a2: 0<e;
a4: now let n,m such that n>=0 & m>=0;
     Rseq.(n,m) = r by a1;
     hence |. Rseq.(n,m) - r.| < e by a2,COMPLEX1:44;
    end;
    reconsider N = 0 as Nat;
