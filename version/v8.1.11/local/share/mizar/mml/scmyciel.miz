        end;
        suppose that A129: x in r.d and A130: y in d;
          consider x1 being Element of uG such that
        A131: x = [x1,uG] and
        A132: x1 in d by A129,A122;
          per cases;
          suppose x1 = y;
            hence {x,y} nin MG by A131,Th100;
