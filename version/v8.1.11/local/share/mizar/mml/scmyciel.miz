  A1: a = V and card V <= 2;
     a = {} or a = {x} or a = {y} or a = {x,y} by A1,ZFMISC_1:36;
   hence thesis by ENUMSET1:def 2;
  end;
A2: {x,y} = Vertices CompleteSGraph {x,y} by Lm1;
A3: x in {x,y} by TARSKI:def 2;
A4: y in {x,y} by TARSKI:def 2;
A5: card {x,y} <= 2 by CARD_2:50;
