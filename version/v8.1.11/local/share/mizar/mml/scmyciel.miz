    end;
    thus uMG c= union E proof
      let a be object;
      assume a in uMG;
        then consider Y being set such that
    A42: a in Y and
    A43: Y in MG by TARSKI:def 4;
    A44: a in uG implies a in union E proof
          assume a in uG;
