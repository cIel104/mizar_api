       by A12,A13,A14,A1,A2,A3,A4,A5,Lm11,LIMFUNC2:12;
    Intf is_right_divergent_to-infty_in a by A14,A16,LIMFUNC2:12;
    hence left_improper_integral(f,a,c) = -infty by A12,A13,A26,Def3;
