  & ext_right_integral(f,c,d) = integral(f,c,d)
proof
   let f be PartFunc of REAL,REAL, a,b be Real;
   assume that
A1: ].a,b.] c= dom f and
A2: f is_left_ext_Riemann_integrable_on a,b;

   hereby let c,d be Real;
