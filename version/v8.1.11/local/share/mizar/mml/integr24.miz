    right_improper_integral(f-g,a,b)
     = right_improper_integral(f,a,b) + right_improper_integral(-g,a,b)
       by A1,A2,A3,A8,A4,A9,A10,A12,Th58
    .= right_improper_integral(f,a,b) + (-right_improper_integral(g,a,b))
       by A1,A3,A5,Th56;
    hence right_improper_integral(f-g,a,b)
      = right_improper_integral(f,a,b) - right_improper_integral(g,a,b)
        by XXREAL_3:def 4;
