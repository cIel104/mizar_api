    assume
A3:  a < d <= b; then
    per cases by XXREAL_0:1;
    suppose d = b;
     hence thesis by A2;
    end;
    suppose
A4:  d < b; then
     [.a,d.] c= [.a,b.[ by XXREAL_1:43; then
     ['a,d'] c= [.a,b.[ by A3,INTEGRA5:def 3; then
A5:  ['a,d'] c= dom f by A1;
