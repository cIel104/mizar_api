A178:     2 in dom E by A167,A165,A166,FINSEQ_3:25;
          1 in Seg len E by A167,A165,A169,FINSEQ_1:1;
          then
A179:     1 in dom E by FINSEQ_1:def 3;
          divset(S2.n,1) =
          [. lower_bound divset(S2.n,1),upper_bound divset(S2.n,1) .]
