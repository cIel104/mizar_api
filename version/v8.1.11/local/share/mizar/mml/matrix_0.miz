  end;
  then reconsider M as FinSequence of (D())* by FINSEQ_1:def 4;
  ex n st for x st x in rng M ex p being FinSequence of D() st x=p & len p = n
  proof
    take m();
    let x;
    assume x in rng M;
    then consider i be Nat such that
A21: i in dom M and
A22: M.i = x by FINSEQ_2:10;
