A26:  k in dom p and
A27:  p.k =z by FINSEQ_2:10;
A28:  k in Seg len p by A26,FINSEQ_1:def 3;
      then
A29:  [i,k] in [:Seg n(),Seg m():] by A9,A21,A24,ZFMISC_1:87;
      z = f.(i,k) by A24,A25,A27,A28;
