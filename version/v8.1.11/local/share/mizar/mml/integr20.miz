A93:  dom h = A by FUNCT_2:def 1;
A94:  divset(T.m,j) c= A by A86,INTEGRA1:8;
A95:   t in divset(T.n,i) & t in divset(T.m,j) by A92,XBOOLE_0:def 4; then
